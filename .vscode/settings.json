{
  "cSpell.words": [
    "autosize",
    "codeacademy",
    "Confuzed",
    "desaturated",
    "dweb",
    "frontmatter",
    "gsap",
    "mantine",
    "matterify",
    "Octo",
    "tabler",
    "typecheck",
    "unstorage",
    "vfile",
    "warpjs"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "cssVariables.lookupFiles": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "node_modules/@mantine/core/styles.css"
  ],
  "prettier.proseWrap": "always",
  "cSpell.enabled": true
}
